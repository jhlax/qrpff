#!/usr/bin/perl -lw

@n = (192, 0, 12);
$a = 29;
@m = (@n, 192, @n, 0);

$b = 69;
@o = (0, 0, 12, 0);

@p = (100, 10);
@q = (@p, 106, 4, @p, 10, 100);
$c = 168;
@r = (@q) x 2;

$x = $y = $z = $q = 0;
$i = 51;

@s = (64,72);

for ( 1 .. 16 ) {
    push @x, @s, $a, $b;
    push @x, @s, $a, 72;
    push @x, @s, $a, $b;
    push @x, @s, $a, $c;

    $a ^= $m[ $x++ % 8 ];
    $b ^= $o[ $y++ % 4 ];;
    $c ^= $r[ $z++ % 16 ];
}

for (@x) {
    print $i;
    $i ^= $_;
}
